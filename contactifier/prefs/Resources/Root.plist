			<string>com.stoqn4opm.contactifier.plist</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSStaticTextCell</string>
			<key>label</key>
			<string>Choose Close Match Threshold</string>
		</dict>
		<dict>
			<key>cell</key>
			<string>PSSliderCell</string>
			<key>max</key>
			<integer>100</integer>
			<key>min</key>
			<integer>0</integer>
